    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
       
         String str = scan.nextLine();
        double num = 3.1415;
        int numb = 42;
        
        
        System.out.println("String: "+str);
        System.out.println("Double: "+num);
        System.out.println("Int: "+numb);
        